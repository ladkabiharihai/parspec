	</div>
                                        <script type="text/javascript"> function showProds() {
                        jQuery('#supra').show();
                        if (!jQuery('.moduletable_products').is(":visible")) {
                            jQuery('.moduletable_products').fadeIn(800);
                        }
                        jQuery('.moduletable_applications').hide();
                        jQuery('#center-block').hide();

                    }

                    function showApps() {
                        return;
                        jQuery('#supra').show();
                        if (!jQuery('.moduletable_applications').is(":visible")) {
                            jQuery('.moduletable_applications').fadeIn(800);
                        }
                        jQuery('.moduletable_products').hide();
                        jQuery('#center-block').hide();
                    }

                    function showMain() {
                        //jQuery('.moduletable_products').hide()
                        jQuery('#supra').hide();
                        jQuery('#center-block').show();
                    }

                    // Code for hover/out on the products link & menu
                    jQuery('.moduletable_header_products').hover(showProds, showMain);
                    jQuery('.moduletable_products').hover(showProds, showMain);

                    // Code for hover/out on the applications link & menu
                    jQuery('.moduletable_header_applications').hover(showApps, showMain);
                    jQuery('.moduletable_applications').hover(showApps, showMain);

                    jQuery('.close').click(function () {
                        jQuery('.moduletable_products').hide();
                        jQuery('.moduletable_applications').hide();
                        jQuery('#supra').hide();
                        jQuery('#center-block').show();

                    });


                    jQuery(document).ready(function ($) {
                // Add first and last menu item classes
                        $('ul.first-child li:first-child').addClass('first_item');
                        $('ul.last-child li:last-child').addClass('last_item');
                    });

                </script>


