<script>
  jQuery(document).ready(function(){
      jQuery('#subscribe-to-newsletter h3').on('click',function(){
      
      if (jQuery(this).next("#newsletter-fly-out").is(':visible')) {
              jQuery(this).next("#newsletter-fly-out").slideUp();
              jQuery(this).children("#toggle-icon").html("&#10010;");
          } else {
              jQuery(this).next("#newsletter-fly-out").slideDown();
              jQuery(this).children("#toggle-icon").html("&#10006;");
          }     
      });
  });
  </script>
