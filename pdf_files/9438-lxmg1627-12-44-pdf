                  }
                  else {
                    html += '<div class="youtube-info" onclick="youtubeModal' + idx + '.style.display = \'block\'" style="padding: 5px;cursor: pointer"><span>' + itm.snippet.title + '</span></div>';
                  }

                  html += '</div>' +
                    '</div>';
                }
				
                jQuery('.homeBox-info .videos').html(html);
              }
            });
