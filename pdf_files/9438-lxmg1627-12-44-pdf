        function stopYoutube(idx) {
          var iframe = document.getElementsByClassName('youtubeIframe')[idx];
          iframe.src = iframe.src;
        }

        function YouTube() {
          jQuery(document).ready(function () {
            var feed = "https://www.googleapis.com/youtube/v3/search?order=date&part=snippet&channelId=UCjXb_4SkQYHXS19UxS4w0cw&maxResults=7&type=video&key=AIzaSyA5AG2PDbo2-B2ZvZXMewpbNkd6ExP2pQ0";

