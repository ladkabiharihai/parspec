              dataType: "jsonp",
              success : function (data) {

                var html = '';

                var itm = {};

                for (var idx = 0; idx < data.items.length; idx++) {
                  itm = data.items[idx];
				  

                  html += '<div class="youtube-video" style="border-bottom: 1px dotted #e1e8ed;padding:3px;float:left;">' +
                    '<div class="youtube-link">' +
                    '<div class="youtube-image" style="float:left;width:33%;padding:10px;vertical-align:middle;">';
					

                  if (isIE () && isIE () < 9) {
                    html += '<a target="_blank" href="https://www.youtube.com/watch?v=' + itm.id.videoId + '"><img width="100%" src="' + itm.snippet.thumbnails['high'].url + '" /></a>';
                  }
                  else {
                    html += '<img id="youtubeThumbnail' + idx + '" width="100%" onclick="youtubeModal' + idx + '.style.display = \'block\'" src="' + itm.snippet.thumbnails['high'].url + '" alt="' + itm.snippet.title + '" />' +
                      '<div id="youtubeModal' + idx + '" class="modal" onclick="document.getElementById(\'youtubeModal' + idx + '\').style.display=\'none\';stopYoutube(' + idx + ');">' +
                      '<div id="video-frame">' +
                      '<span id="youtubeClose' + idx + '" onclick="document.getElementById(\'youtubeModal' + idx + '\').style.display=\'none\';stopYoutube(' + idx + ');">&times;</span>' +
                      '<iframe class="youtubeIframe" src="https://www.youtube.com/embed/' + itm.id.videoId + '" frameborder="0" allowfullscreen></iframe>' +
                      '</div>' +
